   "source": [
    "# Write a program to find the sum of multiples of 5 upto n natural numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "72b21e43-db0e-4c60-ae5f-54cc51eed7c3",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number :  35\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of multiples of 5 upto 35 is : 140\n"
     ]
    }
   ],
   "source": [
    "n = int(input('Enter a number : '))\n",
    "\n",
    "sum = 0\n",
    "\n",
    "for i in range(5, n+1):\n",
    "    if ( (i % 5) == 0 ):\n",
    "        sum += i\n",
    "\n",
    "print(f'Sum of multiples of 5 upto {n} is : {sum}')"
   ]
  }
 ],
