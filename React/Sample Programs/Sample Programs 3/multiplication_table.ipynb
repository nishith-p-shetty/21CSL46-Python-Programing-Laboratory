   ],
   "source": [
    "n = int(input('Enter a number : '))\n",
    "\n",
    "print(f'Multiplication table of {n}')\n",
    "for i in range(1, 11):\n",
    "    print(f'{n} * {i} = {n * i}')"
   ]
  }
