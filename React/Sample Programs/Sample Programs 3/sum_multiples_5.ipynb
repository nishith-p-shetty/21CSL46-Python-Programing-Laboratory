   "source": [
    "# Write a python program to add all the number which are divisible by 5 for a given limit"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "2c93b28e-c0df-4b71-86af-a87de93e7d76",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number :  55\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of multiples of 5 upto 55 is : 330\n"
     ]
    }
   ],
   "source": [
    "n = int(input('Enter a number : '))\n",
    "\n",
    "sum = 0\n",
    "\n",
    "for i in range(5, n+1):\n",
    "    if ( (i % 5) == 0 ):\n",
    "        sum += i\n",
    "\n",
    "print(f'Sum of multiples of 5 upto {n} is : {sum}')"
   ]
  }
 ],
