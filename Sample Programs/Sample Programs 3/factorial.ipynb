   "source": [
    "# Write a program to find factorial of a given number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "e0de4d8f-6a9b-4ca4-904c-a8e2756871a0",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number :  5\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Factorial of 5 is 120\n"
     ]
    }
   ],
   "source": [
    "n = int(input('Enter a number : '))\n",
    "\n",
    "factorial = 1\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    factorial *= i\n",
    "\n",
    "print(f'Factorial of {n} is {factorial}')"
   ]
  }
 ],
